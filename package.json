{
  "name": "facebook-pixel-hunt",
  "version": "0.1.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --config-enable-developer-mode",
    "lint": "eslint . && web-ext lint",
    "package": "npm run build && web-ext build --overwrite-dest --ignore-files \"./tests/**/*\" --ignore-files \"./src/**/*\" --ignore-files \"./rollup.config.js\"",
    "start": "npm run build && web-ext run",
    "start-dev": "npm run dev && web-ext run",
    "test-integration": "npm run package && mv web-ext-artifacts/*.zip web-ext-artifacts/study.xpi && mocha --timeout 30000 \"./tests/integration/*.js\"",
    "watch": "npm-run-all --parallel watch-raw watch-bundled",
    "watch-raw": "npm run dev -- -w",
    "watch-bundled": "web-ext run --target chromium --watch-file dist/background.js"
  },
  "devDependencies": {
    "@mozilla/rally": "github:mozilla-rally/rally-sdk#a62dbb46d46d646acb41e51085eda7ca278479c9",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@themarkup/blacklight-collector": "^2.1.11",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "geckodriver": "^1.22.3",
    "globby": "^11.0.3",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.56.2",
    "rollup-plugin-copy": "^3.4.0",
    "selenium-webdriver": "^4.0.0-beta.4",
    "web-ext": "^6.4.0",
    "webextension-polyfill": "^0.8.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Finding Facebook Pixels",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla-rally/facebook-pixel-hunt.git"
  },
  "keywords": [],
  "author": "Mozilla",
  "license": "MPL-2.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/mozilla-rally/facebook-pixel-hunt/issues"
  },
  "homepage": "https://github.com/mozilla-rally/facebook-pixel-hunt#readme",
  "dependencies": {
  }
}
